package com.loiane.cursojava.lab2833;

public class Lampada {
	
	private String comodo;
	private int potencia;
	private String tipo;
	private String corDaLuz;	
	
	public String getComodo() {
		return comodo;
	}

	public void setComodo(String comodo) {
		this.comodo = comodo;
	}

	public int getPotencia() {
		return potencia;
	}

	public void setPotencia(int potencia) {
		this.potencia = potencia;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public String getCorDaLuz() {
		return corDaLuz;
	}

	public void setCorDaLuz(String corDaLuz) {
		this.corDaLuz = corDaLuz;
	}

	
	String ligarLampada() {
		
		String statusLampada = "Está Ligada";
		 
		return statusLampada; 
	}
	
	String desligarLampada() {
		
		String statusLampada = "Está Desligada";
		 
		return statusLampada; 
	}
	
}
