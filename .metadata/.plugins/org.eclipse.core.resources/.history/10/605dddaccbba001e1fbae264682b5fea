package com.loiane.cursojava.lab34;

public class Exercicio2 {
	

	public static void main(String[] args) {
		
		System.out.println("Soma: " + Calculadora.somar(2500, 3890));
		System.out.println("Soma: " + Calculadora.subtrair(2500, 3890));
		System.out.println("Soma: " + Calculadora.multiplicar(2500, 3890));
		System.out.println("Soma: " + Calculadora.dividir(2500, 3890));
		System.out.println("Soma: " + Calculadora.elevar(2500, 2));
	}

}
