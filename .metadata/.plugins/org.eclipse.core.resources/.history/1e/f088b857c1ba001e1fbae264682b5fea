package com.loiane.cursojava.lab2833;

public class Lampada {
	
	String comodo;
	int potencia;
	String tipo;
	String corDaLuz;
	
	
	String ligarLampada() {
		
		String statusLampada = "Está Ligada";
		 
		return statusLampada; 
	}
	
	String desligarLampada() {
		
		String statusLampada = "Está Desligada";
		 
		return statusLampada; 
	}
	
}
